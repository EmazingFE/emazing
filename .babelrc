{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["last 2 versions", "Chrome >= 50"]
      },
      "test": {
        "presets": [
          ["@babel/preset-env", { "targets": { "node": 8 }}]
        ]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-vue-jsx",
    "transform-runtime",
    "syntax-dynamic-import"
  ]
}
